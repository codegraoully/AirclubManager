    <a href="{{ path('qualification_index') }}" class="btn-primary btn">Retour</a>

    <a href="{{ path('qualification_edit', {'id': qualification.id}) }}" class="btn btn-primary">Modifier</a>

    {{ include('qualification/_delete_form.html.twig') }}
{% endblock %}
