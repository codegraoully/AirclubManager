{% extends 'base.html.twig' %}

{% block title %}Qualification{% endblock %}

{% block body %}
    <h1>Qualification</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ qualification.id }}</td>
        </tr>
        <tr>
            <th>Libelle</th>
            <td>{{ qualification.libelle }}</td>
